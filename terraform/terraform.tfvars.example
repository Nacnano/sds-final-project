# Terraform Variables Override Example
# Copy this file to terraform.tfvars and customize the values

# Kubernetes Configuration
# namespace = "microservices"
# kube_context = "kubernetes-admin@kubernetes"

# Docker Registry
# registry_url = "192.168.0.106:5000"

# Node Configuration
# node_hostname = "warissara-virtualbox"

# Database Credentials
# postgres_user = "postgres"
# postgres_password = "your-secure-password-here"

# JWT Configuration
# jwt_secret = "your-secure-jwt-secret-here"

# PgAdmin Configuration
# pgadmin_email = "admin@example.com"
# pgadmin_password = "your-secure-pgadmin-password-here"

# Service URLs
# frontend_url = "http://192.168.0.106:30002"

# Replica Counts
# api_gateway_replicas = 2
# shrine_service_replicas = 2
# location_service_replicas = 2
# frontend_replicas = 2
